{
	"extends": ["eslint:recommended", "prettier"],
	"plugins": ["prettier"],
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": 2018
	},
	"env": {
		"node": true,
		"es6": true,
		"jest": true
	},
	"rules": {
		"no-console": "warn",
		"no-unused-vars": "warn",
		"prettier/prettier": "error"
	}
}
